{
  "name": "transero",
  "version": "1.0.0",
  "description": "a discord translating bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js",
    "checkFiles": "npx checks-files-js ./dist/ --ignore index.js"
  },
  "keywords": [
    "discord.js",
    "bot",
    "translate",
    "language",
    "dictionary"
  ],
  "author": "Tyo Nirwansyah",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.12.1",
    "@types/randomcolor": "^0.5.5",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "checks-files-js": "^1.0.2",
    "eslint": "^7.28.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^7.0.0",
    "axios": "^0.21.1",
    "country-quiz": "^0.5.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "iso-3166-1": "^2.0.1",
    "iso-639-1": "^2.1.9",
    "randomcolor": "^0.6.2"
  },
  "engines": {
    "node": "14.16.0"
  }
